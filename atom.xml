---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

  <title>JFDI</title>
  <subtitle>Anna Shipman's blog</subtitle>
  <link href="http://www.annashipman.co.uk/atom.xml" rel="self"/>
  <link href="http://www.annashipman.co.uk/"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://www.annashipman.co.uk/</id>
  <author>
    <name>Anna Shipman</name>
  </author>

  {% for post in site.posts %}
  <entry>
    <title>{{ post.title }}</title>
    <link href="http://www.annashipman.co.uk{{ post.url }}"/>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <id>http://www.annashipman.co.uk{{ post.url }}</id>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}

</feed>
